<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Journalife.ViewEntryPage">
    <ContentPage.Content>
        <ScrollView>
            <StackLayout>
                <Label Font="Bold, 14" Margin="2" Text="Date:" />
                <Label Margin="2" x:Name="data" />
                <Label Font="Bold, 14" Margin="2" Text="Gratitudes:" />
                <Label Margin="2" x:Name="g1" />
                <Label Margin="2" x:Name="g2" />
                <Label Margin="2" x:Name="g3" />
                <Label Font="Bold, 14" Margin="2" Text="Journal:" />
                <Label Margin="2" x:Name="diary" />
            </StackLayout>
        </ScrollView>
        
    </ContentPage.Content>
</ContentPage>